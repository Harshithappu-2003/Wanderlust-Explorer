<%if(locals.success && locals.success.length){ %>
<div class="alert alert-info alert-dismissible fade show flash" role="alert">
    <%=success%>
   <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<%}%>
<%if(locals.error && locals.error.length){ %>
<div class=" alert alert-warning alert-dismissible fade show flash" role="alert">
    <%=error%>
   <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<%}%>